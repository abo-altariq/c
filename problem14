#include<iostream>
#include<string>
using namespace std;

void ReadNumber(int& num1, int& num2)
{
	cout << "enter num1" << endl;
	cin >> num1;

		cout << "enter num2" << endl;
	cin >> num2;
}

void PrintNumber(int num1, int num2)
{
	cout << "num1=" << num1 << endl;
	cout << "num2=" << num2 << endl;
}
void SwapNumber(int &num1, int &num2)
{
	int temp;
	temp = num1;
	num1 = num2;
	num2 = temp;
}

void PrintSwapNumber(int num1,int num2)


{
	cout << "aftar swap" << endl;
	cout << "num1=" << num1 << endl;
	cout << "num2=" << num2 << endl;

}

int main()
{
	int num1, num2;
	ReadNumber(num1, num2);
	PrintNumber(num1, num2);
	SwapNumber(num1, num2);
	PrintSwapNumber(num1, num2);
	return 0;
	
}
